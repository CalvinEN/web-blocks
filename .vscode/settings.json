// Place your settings in this file to overwrite default and user settings.
{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "files.trimTrailingWhitespace": false,
  "files.exclude": {
    "build": true,
    ".idea": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/*.map": true,
    "**/*.js": {
      "when": "$(basename).ts"
    }
  }
}